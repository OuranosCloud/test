task.spawn(function()
    while task.wait() do
        local remote = game:GetService("ReplicatedStorage").Network["Mailbox: Get"]:InvokeServer()
        if remote.Inbox ~= nil then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(141, 24, -348)
            task.wait(3)
            for i, v in pairs(game:GetService("ReplicatedStorage").Network["Mailbox: Get"]:InvokeServer().Inbox) do
                    
                    local ohTable1 = {
                        [1] = v.uuid
                    }
                    
                    game:GetService("ReplicatedStorage").Network["Mailbox: Claim"]:InvokeServer(ohTable1)
            end
        end
        task.wait(60)
    end
end)
